<div class="inner cover">
	<h1 class="cover-heading"> Tech News </h1>

	<section>
		<div class="row">
			<% @tweets.each do |tweet| %>
					<div class="col-md-6">
						<div class="tech-news">
							<article>
								<div class="card">
									<%= tweet %>
								</div>
							</article>
						</div>
					</div>
			<% end %>
		</div>
	</section>
</div>